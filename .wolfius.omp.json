{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "❯❯ ",
    "foreground": "magenta",
    "background": "transparent"
  },
  "transient_prompt": {
    "foreground": "#dd464c",
    "template": "{{now | date \"3:04:05 pm\"}} ❯ "
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "windows": "\ue62a"
          },
          "template": "❯ {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#ffffff",
          "type": "os",
          "style": "plain"
        },
        {
          "options": {
            "style": "full"
          },
          "template": "[\uf07b {{ .Path }}]",
          "foreground": "#fdcc59",
          "type": "path",
          "style": "plain"
        },
        {
          "template": "[\ued0d {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}]",
          "foreground": "#149b93",
          "options": {
            "fetch_package_manager": true
          },
          "type": "node",
          "style": "plain"
        },
        {
          "options": {
            "fetch_version": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#1290bf",
          "type": "python",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "always_enabled": true
          },
          "template": "{{ .FormattedMs }} ",
          "powerline_symbol": "\ue0b2",
          "foreground": "#83769c",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "template": " ⚡",
          "foreground": "#86BBD8",
          "type": "root",
          "style": "plain"
        },
        {
          "template": "{{ .UserName }} ",
          "foreground": "#86BBD8",
          "type": "session",
          "style": "plain"
        },
        {
          "options": {
            "time_format": "3:04:05 PM"
          },
          "template": "[{{ .CurrentDate | date .Format }}]",
          "foreground": "#1290BF",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8fc13e",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}",
          "foreground": "#b9b5b8",
          "type": "git",
          "style": "plain",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FCA17D{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#149b93{{ end }}",
            "{{ if gt .Behind 0 }}#c85e7c{{ end }}"
          ]
        },
        {
          "template": " ❯",
          "foreground": "#8fc13e",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "auto_upgrade": true,
  "final_space": true
}
